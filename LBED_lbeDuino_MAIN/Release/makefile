# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: cpp_test.axf

# Tool invocations
cpp_test.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU C++ Linker'
	arm-none-eabi-c++ -nostdlib -Xlinker -Map="cpp_test.map" -Xlinker --gc-sections -mcpu=cortex-m0 -mthumb -T "cpp_test_Release.ld" -o"cpp_test.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(OBJS)$(C++_DEPS)$(C_DEPS)$(CC_DEPS)$(CPP_DEPS)$(EXECUTABLES)$(CXX_DEPS)$(C_UPPER_DEPS) cpp_test.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "cpp_test.axf"; # arm-none-eabi-objcopy -O binary "cpp_test.axf" "cpp_test.bin" ; checksum -p LPC1114FN_102 -d "cpp_test.bin";
	-@echo ' '

.PHONY: all clean dependents
.SECONDARY: post-build

